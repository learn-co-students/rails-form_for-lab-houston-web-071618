<h1>School Class Edit</h1>
<%= form_for @school_class do |f| %>
  <p>School Class Title: <%= f.text_field :title, id: "school_class_title"%></p>
  <p>School Class Room Number: <%= f.number_field :room_number, id: "school_class_room_number" %></p>
  <p><%= f.submit "Update School class" %></p>
<% end %>
