<h1>School Class Form</h1>
<p>Find me in app/views/school_classes/new.html.erb</p>

<%= form_for @new do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />

  <%= f.label :room_number %>:
  <%= f.number_field :room_number %><br />

  <%= f.submit "Create School Class", {:id => "Create School class"} %>
<% end %>
